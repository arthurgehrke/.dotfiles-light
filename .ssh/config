Host *
  UserKnownHostsFile ~/.ssh/known_hosts
  UpdateHostKeys yes
  Compression yes
  AddKeysToAgent yes
  # ForwardX11 yes
  IdentitiesOnly yes
  # ServerAliveInterval 60
  # ServerAliveCountMax 5
  TCPKeepAlive yes
  ControlMaster auto
  # ControlPath ~/.ssh/sockets/%r@%h-%p
  ControlPersist 1h
  ConnectTimeout 1800
  LogLevel VERBOSE
  ControlPersist yes

Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/GITHUB-MAC-AIR-KEY

